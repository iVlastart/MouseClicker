<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="sprites\cheese.png" />
    <None Remove="sprites\mouse.png" />
    <None Remove="sprites\mousetrap.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="sprites\cheese.png" />
    <Resource Include="sprites\mouse.png" />
    <Resource Include="sprites\mousetrap.png" />
  </ItemGroup>

</Project>
